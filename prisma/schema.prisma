// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Country {
  id        Int    @id @default(autoincrement())
  name      String @unique
  flag      String
  isoCode   String
  latitude  Float
  longitude Float
}
